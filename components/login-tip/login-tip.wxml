<!--components/login-tip/login-tip.wxml-->
<view class="login-tip-container {{fixed ? 'fixed' : ''}}">
  <view class="login-tip">
    <view class="login-tip-text">{{tipText || '登录后查看更多内容'}}</view>
    <button class="login-btn" bindtap="onLoginTap">{{buttonText || '一键登录'}}</button>
  </view>
</view> 
